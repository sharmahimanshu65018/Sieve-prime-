int binarysearch(int arr[], int n, int k) 
{ 
    int l,h;
    l=0;
    h=n-1;
    int mid;
    while(l<=h)
    {
        mid=l+h;
        mid/=2;
        if(arr[mid]>k)
        {
            h=mid-1;
        }
        else if(arr[mid]<k)
        {
            l=mid+1;
        }
        else
        {
            return 1;
        }
    }
    return -1;
}
